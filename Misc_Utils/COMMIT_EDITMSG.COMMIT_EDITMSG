-static void *Take_Item(PBQueue *queue){
-	assert(queue);
-	PBQ_Node *node = queue->head;
-	assert(node);
-	void *item = queue->head->item;
-	queue->head = queue->head->next;
-	free(node);
-	queue->size--;
-	return item;
