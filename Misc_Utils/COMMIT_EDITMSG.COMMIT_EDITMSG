-		pthread_rwlock_unlock(list->manipulating_list);
-		return 1;
-	}
-	if(list->current == list->head) add_as_head(list, node);
-	else add_before(list, list->current, node);
-	list->is_sorted = 0;
-	pthread_rwlock_unlock(list->manipulating_iterator);
-	pthread_rwlock_unlock(list->manipulating_list);
-	return 1;
