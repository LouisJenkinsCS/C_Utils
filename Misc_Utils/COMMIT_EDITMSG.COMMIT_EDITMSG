-		}
-	}
-	void *item = Take_Item(queue);
-	//printf("Timed_Dequeue: Queue Size is %d\n", queue->size);
-	pthread_cond_signal(queue->is_not_full);
-	pthread_mutex_unlock(queue->adding_or_removing_elements);
-	return item;
