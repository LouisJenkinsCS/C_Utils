-	pthread_mutex_lock(queue->adding_or_removing_elements);
-	struct timespec timeout;
-	clock_gettime(CLOCK_REALTIME, &timeout);
-	timeout.tv_sec += seconds;
-	while(!queue->size){ 
-		//printf("Timed_Dequeue: Condition Waiting!\n");
