diff --git a/Data_Structures/Tests/DS_Hash_Map_Test.c b/Data_Structures/Tests/DS_Hash_Map_Test.c
new file mode 100644
index 0000000..0290586
--- /dev/null
+++ b/Data_Structures/Tests/DS_Hash_Map_Test.c
@@ -0,0 +1,69 @@
+#include <DS_Hash_Map.h>
+#include <string.h>
+#include <MU_Logger.h>
+
+static MU_Logger_t *logger = NULL;
+const int buckets = 31;
+const int synchronized = 0;
+
+int main(void){
+	logger = MU_Logger_create("DS_Hash_Map_Test.log", "w", MU_ALL);
+	char *keys[] = {
+		"Hello World",
+		"How are you",
+		"Doing today?",
+		"Fine I hope!",
+		"Because it is a good day",
+		"To be alive!"
+	};
+	char *values[] = {
+		"Goodbye World",
+		"It was nice knowing you",
+		"In my short time on Earth",
+		"But it is time for me to go!",
+		"My people need me",
+		"And I them!"
+	};
+	int i = 0;
+	MU_LOG_VERBOSE(logger, "Logging all Key-Value pairs!\n");
+	char *all_pairs, *old_str;
+	asprintf(&all_pairs, "\n{\n ");
+	for(; i < 6; i++){
+		old_str = all_pairs;
+		asprintf(&all_pairs, "%s%s : %s,\n", all_pairs, keys[i], values[i]);
+		free(old_str);
+	}
+	old_str = all_pairs;
+	asprintf(&all_pairs, "%s}", all_pairs);
+	free(old_str);
+	MU_LOG_VERBOSE(logger, "%s", all_pairs);
+	MU_LOG_INFO(logger, "Creating Hash Map...\n");
+	DS_Hash_Map_t *map = DS_Hash_Map_create(buckets, synchronized);
+	MU_ASSERT(map, logger, "DS_Hash_Map_create: \"Was unable to allocate hash map!\"\n");
+	MU_LOG_INFO(logger, "Adding all key-value pairs to hash map...\n");
+	for(i = 0; i < 6; i++){
+		int was_added = DS_Hash_Map_add(map, keys[i], values[i]);
+		MU_ASSERT(was_added, logger, "DS_Hash_Map_add: \"Was unable to add key: \"%s\" with value: \"%s\"!\"\n", keys[i], values[i]);
+	}
+	MU_LOG_INFO(logger, "Retrieivng all values from keys from hash map...\n");
+	for(i--; i > 0; i--){
+		char *value_retrieved = DS_Hash_Map_get(map, keys[i]);
+		MU_ASSERT(value_retrieved && strcmp(value_retrieved, values[i]) == 0, logger,
+			"DS_Hash_Map_get: \"Was unable to retrieve the right value from key: \"%s\";Expected: \"%s\", but received \"%s\"!\"\n",
+			keys[i], values[i], value_retrieved);
+	}
+	MU_LOG_INFO(logger, "Removing Key-Value pair (\"%s\" : \"%s\")...\n", keys[3], values[3]);
+	MU_ASSERT(DS_Hash_Map_remove(map, keys[3], NULL), logger, "DS_Hash_Map_remove: \"Was unable to remove key: \"%s\"!\"\n", keys[3]);
+	MU_LOG_INFO(logger, "Testing for removal of Key-Value pair...\n");
+	void *was_found = DS_Hash_Map_get(map, keys[3]);
+	MU_ASSERT(!was_found, logger, "DS_Hash_Map_get: \"Was unable to correctly remove key: \"%s\"!\n", keys[3]);
+	MU_LOG_INFO(logger, "Testing for adding duplicate key...\n");
+	MU_ASSERT(!DS_Hash_Map_add(map, keys[2], values[2]), logger, "DS_Hash_Map_add: \"Was unable to prevent duplicate key: \"%s\" and value: \"%s\"!\"\n",
+		keys[2], values[2]);
+	MU_LOG_INFO(logger, "Testing retrieval of key by value...\n");
+	MU_ASSERT(strcmp(keys[1], DS_Hash_Map_contains(map, values[1], (void *)strcmp)) == 0, logger, "DS_Hash_Map_contains: \"Was unable to find value inside of map!\"\n");
+	MU_LOG_INFO(logger, "Destroy Hash Map...\n");
+	MU_ASSERT(DS_Hash_Map_destroy(map, NULL), logger, "DS_Hash_Map_destroy: \"Was unable to destroy map!\"\n");
+	MU_LOG_INFO(logger, "Success!\n");
+	return 1;
+}
